{
    "env": {
        "browser": true,
        "es2021": true
    },
    "settings": {
        "react":{
            "version":"detect"
        }
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "pulgin:react-hooks/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off",
        "@typescript-eslint/explicit-module-boundary-types": "off"
    }
}
